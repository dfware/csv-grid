{
    "name": "dfware/csv-grid",
    "description": "Yii2 extension data export to CSV file",
    "keywords": ["yii2", "csv", "export", "large data"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/dfware/csv-grid/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/dfware/csv-grid/wiki",
        "source": "https://github.com/dfware/csv-grid"
    },
    "authors": [
        {
            "name": "Pavel Karelin",
            "email": "devpastet@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.27 || ^5.0 || ^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "yii2tech\\csvgrid\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}